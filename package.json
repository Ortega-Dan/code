{
    "name": "serenade",
    "displayName": "Serenade",
    "description": "Code with voice. Learn more at https://serenade.ai.",
    "publisher": "serenadeai",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.serenadeEnable",
        "onCommand:extension.serenadeShowTranscriptInput"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.serenadeEnable",
                "title": "Enable Serenade"
            },
            {
                "command": "extension.serenadeShowTranscriptInput",
                "title": "Type a Serenade command"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "npm run tsc ; npm run scss",
        "tsc": "tsc -p ./",
        "scss": "node-sass-chokidar ./src -o ./out",
        "watch": "node-sass-chokidar ./src -o ./out --watch & tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "decompress": "^4.2.0",
        "mkdirp": "0.5.1",
        "request": "^2.88.0",
        "request-progress": "^3.0.0",
        "rimraf": "2.6.3",
        "tree-kill": "1.2.1"
    },
    "devDependencies": {
        "@types/decompress": "^4.2.3",
        "@types/mkdirp": "^0.5.2",
        "@types/mocha": "^2.2.42",
        "@types/node": "^10.12.21",
        "@types/request": "^2.48.1",
        "@types/rimraf": "^2.0.2",
        "node-sass-chokidar": "^1.3.4",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1",
        "vscode": "^1.1.28"
    }
}
