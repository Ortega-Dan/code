{
    "name": "serenade",
    "description": "Code with voice. Learn more at https://serenade.ai.",
    "displayName": "Serenade",
    "license": "UNLICENSED",
    "publisher": "serenade",
    "repository": "https://github.com/serenadeai/code",
    "version": "1.0.0",
    "extensionKind": "ui",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.serenadeEnable",
        "onCommand:extension.serenadeShowTranscriptInput"
    ],
    "main": "./build/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.serenadeEnable",
                "title": "Enable Serenade"
            },
            {
                "command": "extension.serenadeShowTranscriptInput",
                "title": "Type a Serenade command"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "compile": "sass ../serenade-editor-shared/alternatives-panel.scss ../serenade-editor-shared/build/alternatives-panel.css && webpack --mode production",
        "dev": "webpack --mode development --watch",
        "watch": "",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "mkdirp": "0.5.1",
        "rimraf": "2.6.3",
        "targz": "1.0.1",
        "tree-kill": "1.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.5.0",
        "@babel/preset-env": "^7.5.0",
        "@types/mkdirp": "^0.5.2",
        "@types/mocha": "^2.2.42",
        "@types/node": "^10.14.12",
        "@types/rimraf": "^2.0.2",
        "@types/targz": "^1.0.0",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.0.0",
        "mini-css-extract-plugin": "^0.7.0",
        "node-sass": "^4.12.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "ts-loader": "^6.0.4",
        "tslint": "^5.12.1",
        "typescript": "^3.5.2",
        "vscode": "^1.1.28",
        "webpack": "^4.35.0",
        "webpack-cli": "^3.3.5"
    }
}
