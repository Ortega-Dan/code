{
  "name": "serenade",
  "description": "Code with voice. Learn more at https://serenade.ai.",
  "displayName": "Serenade",
  "license": "MIT",
  "publisher": "serenade",
  "repository": "https://github.com/serenadeai/code",
  "version": "1.2.1",
  "extensionKind": "ui",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": ["Other"],
  "activationEvents": ["*"],
  "main": "./build/extension.js",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "sass ../serenade-editor-shared/src/global.scss ../serenade-editor-shared/build/global.css && webpack --mode production",
    "dev": "webpack --mode development --watch",
    "watch": "",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "mkdirp": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.14.12",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "tslint": "^5.12.1",
    "typescript": "^3.5.2",
    "vscode": "^1.1.28",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  }
}
